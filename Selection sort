def Selection_Sort(arr):
    for i in range(len(arr)):
        min_indeks=i
        for j in range(i + 1, len(arr)):
            if  arr[min_indeks] > arr[j]:
                min_indeks = j
        temp = arr[i]
        arr[i] = arr[min_indeks]
        arr[min_indeks] = temp



list= [14, 33, 27, 10, 35, -1, 45]
print("Array:   ")
print(list)
Selection_Sort(list)
print("Sorted array!")
print(list)
