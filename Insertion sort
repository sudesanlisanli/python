def Insertion_Sort(arr):
    for i in range(1, len(arr)):
       gecici = arr[i]
       j = i - 1
       while j >= 0 and gecici < arr[j]:
           arr[j + 1] = arr[j]
           j =j- 1
       arr[j + 1] = gecici


array= [14, 33, 27, 10, 35, -1, 45]
print("Array:   ")
print(array)
Insertion_Sort(array)
print("Sorted array!")
print(array)
